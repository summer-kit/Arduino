void setup() {
  Serial.begin(9600); //시리얼 통신을 개방하고, 통신 속도를 설정한다.
}

void loop() {
for (int i=0; i<5; i++) {
  Serial.print(i);
}
Serial.println();
for (int i=0; i<5; i++) {
  Serial.println(i);
}
for (int i=0; i<5; i++) {
  Serial.print("Value"+(String)i+" : ");
  Serial.println(i+10);
}
delay(5000);
}
